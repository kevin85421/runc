	"ociVersion": "1.0.2-dev",
	"process": {
		"terminal": true,
		"user": {
			"uid": 0,
			"gid": 0
		},
		"args": [
			"sh",
			"-c",
			"pwd ; cd /dev/shm ; pwd ; pwd ; touch /dev/shm/file ; set -x ; for i in `seq 1 2` ; do mkdir /dev/shm/directory$i dir$i; chmod 777 /dev/shm/file ; ls -la /dev/shm ; sleep 2 ; done"
		],
		"env": [
			"PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
			"TERM=xterm"
		],
		"cwd": "/",
